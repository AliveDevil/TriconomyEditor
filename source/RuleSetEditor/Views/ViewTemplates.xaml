<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:vm="clr-namespace:RuleSetEditor.ViewModels"
					xmlns:local="clr-namespace:RuleSetEditor.Views">
	<DataTemplate DataType="{x:Type vm:LandingPageViewModel}">
		<local:LandingPageView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:RuleSetViewModel}">
		<local:RuleSetView />
	</DataTemplate>
	
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="EffectViews/EffectViewTemplates.xaml" />
		<ResourceDictionary Source="ConditionViews/ConditionViewTemplates.xaml" />
		<ResourceDictionary Source="EventViews/EventViewTemplates.xaml" />
		<ResourceDictionary Source="RuleSetViews/RuleSetViewTemplates.xaml" />
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>
