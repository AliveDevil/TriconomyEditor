<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:vm="clr-namespace:RuleSetEditor.ViewModels.RuleSetViewModels.ElementViewModels"
					xmlns:local="clr-namespace:RuleSetEditor.Views.RuleSetViews.ResourceViews">
	<DataTemplate DataType="{x:Type vm:ResourceViewModel}">
		<local:ResourceView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:ResourceGroupViewModel}">
		<local:ResourceGroupView />
	</DataTemplate>
	<DataTemplate DataType="{x:Type vm:ResourcePartViewModel}">
		<local:ResourcePartView />
	</DataTemplate>
</ResourceDictionary>
